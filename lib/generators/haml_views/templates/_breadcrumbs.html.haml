#breadcrumbs
  %ul#crumbs
    %li
      = link_to t("breadcrums.homepage"), home_index_path
    %li
      = link_to <%= model %>.model_name.human.pluralize, <%= admin_path %><%= items %>_path
    - if @forview
      - if @forview == "NEW"
        %li= t("data.title_new")
      - elsif @forview == "EDIT"
        %li= t("data.title_edit")
      - else
        %li= t("data.title_show")
.clear &nbsp;
